// ActionScript file

	private function ClearBankAddress():void{

		lbBank.text 	= "";
		lbBranch.text	= ""; 
		lbStreet.text	= ""; 
		lbCity.text		= ""; 
		lbSwift.text	= "";		
	}

	private function PopulateBankAddress(xmlBankDetails:XMLList):void{

		lbBank.text 	= xmlBankDetails.BANKA;
		lbBranch.text	= xmlBankDetails.BRNCH;
		lbStreet.text	= xmlBankDetails.STREET;
		lbCity.text		= xmlBankDetails.CITY;
		lbSwift.text	= xmlBankDetails.SWIFT;
	}

