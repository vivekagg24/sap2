package ecs.generalClasses
{
	import flash.events.EventDispatcher;
	
	[Event(name="propertyChange", type="mx.events.PropertyChangeEvent")]
	
	public class MessageSuppression extends EventDispatcher
	{	
		protected var _suppressed:Boolean;		
		
		[Bindable]
		public function set suppression(val:Boolean):void
		{
			_suppressed = val;
		}
		public function get suppression():Boolean
		{
			return _suppressed;
		}
		
		
		
		
		
	}
}