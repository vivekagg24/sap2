<Bar xmlns="sap.m">
  <contentLeft>
    <Button icon="sap-icon://nav-back" visible="{config>showBack}" press="onNavBack" />
  </contentLeft>
  <contentMiddle>

    <SegmentedButton
      id="navButtons"
      selectedKey="{config>/mode}"
      items="{user>/header_nav}">

      <items>
        <SegmentedButtonItem
          icon="{user>icon}"
          text="{user>text}"
          key="{user>key}"
          press="onHeaderNavButtonPress" />
        </items>

    </SegmentedButton>

