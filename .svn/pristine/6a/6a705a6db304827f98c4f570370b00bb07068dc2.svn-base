	            backgroundColor  = "#D4D4D4"
	            scroll           = "true" 
	            enabled          = "true" 
	            layout           = "absolute"
            	width            = "100%" 
            	height           = "100%"	
             	creationComplete = "startApp()" 
				initialize       = "Init()"
				creationPolicy   = "all" 
			    resize           = "onResize(event)"
